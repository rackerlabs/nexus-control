{
    "developer.rackspace.com": [
        {
            "from": "^\\/docs\\/user-guides\\/?$",
            "to": "/docs/user-guides/infrastructure/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Force trailing slash",
            "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
            "to": "/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Blog: Remove .html extension",
            "from": "^\\/blog\\/(.+?)(\\.html)$",
            "to": "/blog/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/devtrial",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/developer-plus\\/?$",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/signup",
            "to": "/cloud",
            "toProtocol": "https",
            "toHostname": "cart.rackspace.com",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/sdks\\/?$",
            "to": "/docs/#sdks",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-cdn\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/cdn/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-autoscale\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/autoscale/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-metrics\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/metrics/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-networks\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/cloud-networks/v2/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/metrics\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/metrics/v2/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-monitoring\\/?(.*)$",
            "to": "/docs/rackspace-monitoring/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/how-to\\/cloud-monitoring\\/?(.*)$",
            "to": "/how-to/rackspace-monitoring/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/logs\\/?$",
            "to": "/",
            "toProtocol": "https",
            "toHostname": "iad1-10096-kibana.es.objectrocket.com",
            "toPort": "20096",
            "rewrite": false,
            "status": 302
        },
        {
          "from": "^/docs/private-cloud/rpc/?$",
          "to": "/docs/private-cloud/rpc/v11"
        },
        {
          "from": "^\\/docs\\/(.+?)\\/getting-started\\/(.*?)",
          "to": "/docs/$1/quickstart/$2",
          "rewrite": false,
          "status": 301
        }
    ]
}
