          "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
          "to": "/$1/",
          "rewrite": false,
          "status": 301
      },
      {
          "from": "^\\/logs\\/?$",
          "to": "/",
          "toProtocol": "https",
          "toHostname": "iad1-10096-kibana.es.objectrocket.com",
