{
    "deconst.horse": [
      {
        "from": "^\\/logs\\/?$",
        "to": "/",
        "toProtocol": "https",
        "toHostname": "/",
        "toPort": "20094",
        "rewrite": false,
        "status": 302
      },
      {
        "description": "Force trailing slash",
        "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
        "to": "/$1/",
        "rewrite": false,
        "status": 301
      }
    ]
  }
  