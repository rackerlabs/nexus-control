{
    "developer.rackspace.com": [
        {
            "from": "^\\/docs\\/user-guides\\/?$",
            "to": "/docs/user-guides/infrastructure/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Force trailing slash",
            "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
            "to": "/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Blog: Remove .html extension",
            "from": "^\\/blog\\/(.+?)(\\.html)$",
            "to": "/blog/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/devtrial",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/developer-plus\\/?$",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/sdks\\/?$",
            "to": "/docs/#sdks",
            "rewrite": false,
            "status": 301
        }
    ]
}
